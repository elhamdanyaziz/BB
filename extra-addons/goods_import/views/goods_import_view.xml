<?xml version="1.0" encoding="UTF-8"?>
<openerp>
	<data>

        <record id="goods_import_tree" model="ir.ui.view">
            <field name="name">goods.import.tree</field>
            <field name="model">goods.import</field>
            <field name="arch" type="xml">
                <tree string="goods import list">
                    <field name="name"/>
                    <field name="partner_id"/>
                    <field name="entry_date" />
                    <field name="import_engagement"/>
                    <field name="export_declaration"/>
                    <field name="date_export_declaration" />
                    <field name="currency_rate"/>
                    <field name="currency_id"/>
                    <field name="receipt_number" />
                    <field name="certificate_marine_insurance_number" />
                    <field name="state"/>
                </tree>
            </field>
        </record>

        <record id="goods_import_form" model="ir.ui.view">
            <field name="name">goods.import.form</field>
            <field name="model">goods.import</field>
            <field name="arch" type="xml">
                <form string="Goods Import">
                    <header>
                        <button name="action_validated" states="draft" string="Valider" type="object" class="oe_highlight" groups="" />
                        <field name="state" widget="statusbar" statusbar_visible="draft,validated" statusbar_colors='{"draft":"blue","validated":"blue"}'/>
                    </header>
                    <h1>
                        <field name="name" class="oe_inline" readonly="0"/>
                    </h1>
                    <group>
                        <group>
                            <field name="partner_id"/>
                            <field name="date"/>
                            <field name="year" invisible="1"/>
                            <field name="entry_date"/>
                            <field name="import_engagement"/>
                            <field name="domiciliation_number"/>
                        </group>
                        <group>
                            <separator colspan="4" string="DUM"/>
                            <field name="export_declaration"/>
                            <field name="date_export_declaration" />
                            <field name="currency_rate"/>
                            <field name="currency_id" invisible="1"/>
                            <separator colspan="4" string="Quittance et Certificat"/>
                            <newline/>
                            <field name="receipt_number" />
                            <field name="certificate_marine_insurance_number" />
                            <field name="amount_bank_charges" />
                            <!--<separator colspan="4" string="Coef Import"/>-->
                            <!--<newline/>-->
                            <!--<field name="import_coef" />-->
                        </group>
                    </group>
                    <notebook>
                        <page string="Achats" readonly="1" invisible="1">
                            <group>
                                <field name="purchase_ids" nolabel="1"/>
                            </group>
                        </page>
                        <page string="Réceptions" readonly="1">
                            <group>
                                <field name="picking_ids" nolabel="1"/>
                            </group>
                        </page>
                        <page string="Factures Fournisseur" readonly="1">
                            <group>
                                <field name="invoice_ids" nolabel="1"/>
                            </group>
                        </page>
                        <page string="coefficients d'Import" readonly="1">
                            <field name="product_coef_ids" nolabel="1">
                                <tree string="coefficients d'Import">
                                    <field name="product_id" />
                                    <field name="import_coef" />
                                </tree>
                            </field>
                        </page>
                        <page string="Informations complémentaires" readonly="1">
                            <group string="Informations générales">
                                <group>
                                    <field name="user_id" />
                                </group>
                                <group>
                                    <field name="company_id" groups="" />
                                </group>
                                <group>
                                    <field name="note" groups="" />
                                </group>
                            </group>
                        </page>
                    </notebook>
                    <group class="oe_subtotal_footer oe_right">
                        <field name="import_amount" widget="monetary" options="{'currency_field': 'currency_id'}"/>
                        <field name="amount_customs_charges" widget="monetary" options="{'currency_field': 'currency_id'}"/>
                        <field name="amount_import_charges" widget="monetary" options="{'currency_field': 'currency_id'}"/>
                        <field name="amount_total" widget="monetary" options="{'currency_field': 'currency_id'}"/>
                    </group>
                </form>
            </field>
        </record>

        <record id="view_goods_import_filter" model="ir.ui.view">
            <field name="name">goods.import.list.select</field>
            <field name="model">goods.import</field>
            <field name="arch" type="xml">
                <search string="Search Goods Import">
                    <field name="name" />
                    <field name="partner_id" operator="child_of"/>
                    <field name="user_id"/>
                    <field name="entry_date"/>
                    <field name="import_engagement"/>
                    <field name="export_declaration"/>
                    <field name="date_export_declaration" />
                    <separator/>
                    <group expand="0" string="Group By">
                        <filter string="Responsbale" domain="[]" context="{'group_by':'user_id'}"/>
                        <filter string="Fournisseur" domain="[]" context="{'group_by':'partner_id'}"/>
                    </group>
               </search>
            </field>
        </record>


        <record model="ir.actions.act_window" id="action_goods_import_tree_view">
            <field name="name">Dossiers d'Import</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">goods.import</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="search_view_id" ref="view_goods_import_filter"/>
        </record>

        <menuitem name="Imports" id="menu_goods_import_root" parent="base.menu_purchase_root" groups="" sequence="0"/>
        <menuitem id="menu_goods_import" parent="menu_goods_import_root" action="action_goods_import_tree_view" sequence="1"/>

        <record model="ir.ui.view" id="purchase_order_form_inherit1">
            <field name="name">purchase.order.form.inherit1</field>
            <field name="model">purchase.order</field>
            <field name="inherit_id" ref="purchase.purchase_order_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='currency_id']" position="after">
					<field name="import_id" invisible="1"/>
                </xpath>
                <xpath expr="//field[@name='minimum_planned_date']" position="after">
					<field name="settlement_date"/>
                </xpath>
                <xpath expr="//page[@string='RFQ &amp; Bid']" position="replace">
                    <page string="RFQ &amp; Bid" invisible="1">
                        <group>
                            <field name="incoterm_id"/>
                            <field name="bid_date"/>
                            <field name="bid_validity"/>
                        </group>
                    </page>
                </xpath>
                <xpath expr="//field[@name='price_subtotal']" position="after">
					<field name="customer_id"/>
                </xpath>
            </field>
        </record>

        <record model="ir.ui.view" id="view_picking_form_inherit1">
            <field name="name">stock.picking.form.inherit1</field>
            <field name="model">stock.picking</field>
            <field name="inherit_id" ref="stock.view_picking_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='partner_id']" position="after">
					<field name="import_id" attrs="{'invisible':[('picking_type_id','!=',1)]}"/>
                </xpath>
            </field>
        </record>

        <record model="ir.ui.view" id="invoice_supplier_form_inherit1">
            <field name="name">account.invoice.supplier.form.inherit1</field>
            <field name="model">account.invoice</field>
            <field name="inherit_id" ref="account.invoice_supplier_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='partner_id']" position="after">
					<field name="import_id"/>
                    <field name="type_service"/>
                </xpath>
                <xpath expr="//field[@name='currency_id']" position="after">
					<field name="currency_rate"/>
                </xpath>
                <!--<xpath expr="//field[@name='product_id']" position="before">-->
					<!--<field name="type_product"/>-->
                <!--</xpath>-->
                <xpath expr="//field[@name='invoice_line']" position="replace">
					<field context="{'invoice_line': invoice_line,'partner_id': partner_id, 'price_type': context.get('price_type') or False, 'type': type}" name="invoice_line">
                        <tree string="Invoice lines" editable="bottom">
                            <field name="sequence" widget="handle"/>
                            <field name="type_product"/>
                            <field name="product_id" on_change="product_id_change(product_id, uos_id, quantity, name, parent.type, parent.partner_id, parent.fiscal_position, price_unit, parent.currency_id, parent.company_id, context)"/>
                            <field name="name"/>
                            <field name="company_id" invisible="1"/>
                            <field name="account_id" groups="account.group_account_user" domain="[('company_id', '=', parent.company_id), ('journal_id', '=', parent.journal_id), ('type', '=', 'other')]" on_change="onchange_account_id(product_id, parent.partner_id, parent.type, parent.fiscal_position,account_id)"/>
                            <field name="account_analytic_id" groups="analytic.group_analytic_accounting" domain="[('type','!=','view'), ('company_id', '=', parent.company_id), ('state','not in',('close','cancelled'))]"/>
                            <field name="quantity"/>
                            <field name="uos_id" groups="product.group_uom" on_change="uos_id_change(product_id, uos_id, quantity, name, parent.type, parent.partner_id, parent.fiscal_position, price_unit, parent.currency_id, parent.company_id, context)"/>
                            <field name="price_unit"/>
                            <field name="discount" groups="sale.group_discount_per_so_line"/>
                            <field name="invoice_line_tax_id" widget="many2many_tags" context="{'type':parent.type}" domain="[('parent_id','=',False),('company_id', '=', parent.company_id)]"/>
                            <field name="price_subtotal"/>
                        </tree>
                    </field>
                </xpath>
                <xpath expr="//field[@name='tax_line']" position="replace">
					<field name="tax_line" context="{'invoice_line':invoice_line}">
                        <tree editable="bottom" string="Taxes">
                            <field name="name"/>
                            <field name="account_id" groups="account.group_account_invoice"/>
                            <field name="account_analytic_id" domain="[('type','&lt;&gt;','view'), ('company_id', '=', parent.company_id)]" groups="analytic.group_analytic_accounting"/>
                            <field name="base" on_change="base_change(base,parent.currency_id,parent.company_id,parent.date_invoice)" readonly="1"/>
                            <field name="amount" on_change="amount_change(amount,parent.currency_id,parent.company_id,parent.date_invoice)"/>
                            <field invisible="True" name="base_amount"/>
                            <field invisible="True" name="tax_amount"/>
                            <field name="factor_base" invisible="True"/>
                            <field name="factor_tax" invisible="True"/>
                        </tree>
                    </field>
                </xpath>
            </field>
        </record>

        <record model="ir.ui.view" id="product_template_form_view_inherit1">
            <field name="name">product.template.common.form.inherit1</field>
            <field name="model">product.template</field>
            <field name="inherit_id" ref="product.product_template_form_view"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='standard_price']" position="after">
