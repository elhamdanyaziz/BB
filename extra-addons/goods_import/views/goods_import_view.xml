                </xpath>
                <xpath expr="//group[@name='status']" position="replace">
                    <group name="status" string="Status" invisible="1">
                        <field name="state"/>
                        <field name="product_manager"/>
                    </group>
                </xpath>
                <xpath expr="//field[@name='seller_ids']" position="replace">
                    <field name="seller_ids" groups="crm_prospecting.group_warehouse"/>
                </xpath>
            </field>
        </record>

        <record model="ir.ui.view" id="product_template_only_form_view_inherit1">
            <field name="name">product.template.product.form.inherit1</field>
            <field name="model">product.template</field>
            <field name="inherit_id" ref="product.product_template_only_form_view"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='ean13']" position="replace">
					<field name="ean13" attrs="{'invisible': [('product_variant_count', '&gt;', 1)]}" invisible="1"/>
