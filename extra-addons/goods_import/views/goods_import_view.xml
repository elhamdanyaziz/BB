        </record>

        <record model="ir.ui.view" id="view_template_property_form_inherit1">
            <field name="name">product.template.stock.property.form.inherit.inherit1</field>
            <field name="model">product.template</field>
            <field name="inherit_id" ref="stock.view_template_property_form"/>
            <field name="arch" type="xml">
                <xpath expr="//group[@name='store']" position="replace">
                </xpath>
                <xpath expr="//field[@name='virtual_available']" position="after">
                    <field name="outgoing_qty"/>
                    <label for="total_reservation_qty"/>
                    <div>
                        <field name="total_reservation_qty" class="oe_inline"/>
                        <button name="action_res_by_section" string="⇒ Réservation BU" type="object" class="oe_link" groups=""/>
                    </div>
                </xpath>
                <xpath expr="//label[@for='qty_available']" position="replace">
                    <label for="qty_available" string="Stock Principal"/>
                </xpath>
                <xpath expr="//label[@for='incoming_qty']" position="replace">
                    <label for="incoming_qty" string="Arrivage"/>
                </xpath>
                <xpath expr="//field[@name='virtual_available']" position="replace">
                    <field name="virtual_available" string="Quantité théorique"/>
                </xpath>
                <xpath expr="//button[@name='%(stock.action_view_change_product_quantity)d']" position="replace">
                    <button name="%(stock.action_view_change_product_quantity)d" string="⇒ Update" type="action" class="oe_link" groups="crm_prospecting.group_warehouse"/>
                </xpath>
                <xpath expr="//button[@name='%(stock.act_make_procurement)d']" position="replace">
                    <button string="⇒ Request Procurement" name="%(stock.act_make_procurement)d" type="action" class="oe_link" groups="crm_prospecting.group_warehouse"/>
                </xpath>

            </field>
        </record>

